{
    "api": { "port": 9999 },
    "swarm": {
            "dev": {
                "host": "http://10.3.152.222",
                "port": 2376
            }
        },
    "servers": [
        {
            "port": 8080,
            "paths": {
                "/": { 
                    "swarm": "dev",
                    "filters": {
                        "compose": {
                            "project": "brett",
                            "service": "site"
                        },
                        "exposedPort": 8080
                    },
                    "headers": {
                        "CustomHeader": "value"
                    }
                },
                "/cms": { 
                    "swarm": "dev",
                    "filters": {
                        "image": "cms:latest"
                    }
                }
            }
        },
        {
            "port": 27017,
            "paths": {
                "/": { 
                    "swarm": "dev",
                    "filters": {
                        "hostname": { "matchType": "compose" },
                        "exposedPort": 27017
                    }
                }
            }
        }]
}
